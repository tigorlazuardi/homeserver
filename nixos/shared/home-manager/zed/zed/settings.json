{
  "context_servers": {
    "grafana-bareksa": {
      "url": "https://mcp-grafana.bareksa.com/sse",
      "enabled": true
    }
  },
  "agent_servers": {
    "claude": {
      "default_mode": "bypassPermissions"
    }
  },
  "completions": {
    "words": "disabled"
  },
  "ssh_connections": [
    {
      "host": "Homeserver",
      "args": [],
      "projects": [
        {
          "paths": [
            "/home/homeserver/dotfiles"
          ]
        }
      ]
    }
  ],
  "load_direnv": "shell_hook",
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "icon_theme": "Catppuccin Mocha",
  "base_keymap": "VSCode",
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Catppuccin Latte (Blur)"
  },
  "languages": {
    "TypeScript": {
      "language_servers": [
        "!vtsls",
        "tsgo",
        "eslint"
      ]
    }
  },
  "vim_mode": true,
  "auto_install_extensions": {
    "catppuccin-blur": true,
    "catppuccin-icons": true,
    "comment": true,
    "dbml": true,
    "emmet": true,
    "fish": true,
    "go": true,
    "hocon": true,
    "http": true,
    "json5": true,
    "jsonl": true,
    "live-server": true,
    "lua": true,
    "make": true,
    "nix": true,
    "tsgo": true,
    "docker": true,
    "toml": true,
    "dockerfile": true,
    "html": true
  }
}
